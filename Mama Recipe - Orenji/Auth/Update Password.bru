meta {
  name: Update Password
  type: http
  seq: 5
}

put {
  url: {{url}}/user/profile/update-password-new
  body: json
  auth: inherit
}

body:json {
  {
      "old_password": "good-password",
      "password": "new-good-password"
  }
  
  // by default using existing Bearer Token that has no expired
}

settings {
  encodeUrl: true
}
